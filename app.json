    },
    "android": {
      "icon": "./assets/appLogo.png",
      "package": "com.gourav2001k.foodntime",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
